<!DOCTYPE html>
<html lang="en-US">
<head>
<meta charset="utf-8">
<title>Categories</title>
<link rel="stylesheet" type="text/css" href="/resources/dwebb29-Stylesheet.css">
</head>
<body>
  <div id="heads">
    <% include partials/header.ejs %>
    <% include partials/user-navigation.ejs %>
  </div>
  <div id="all">
  <main id="content">

      <% allItems.forEach(function(item){ %>
          <article>
          <h3><%= item.itemType %></h3>
          <a href="/item/<%= item._id %>"><%= item.itemName %></a>
          </article>
      <% }) %>

  </main>
    <div id="column">
      <% include partials/site-navigation.ejs %>
      <% include partials/footer.ejs %>
    </div>
  </div>
</body>
</html>
