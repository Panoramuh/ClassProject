<!doctype html>
<html lang="en">

<head>
    <title>TEST</title>
    <meta name="description" content="carSwap" />
	<meta charset="UTF-8">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- <script type="text/javascript" src="/javascript/message.js"></script> -->


</head>

<body style="background-color: grey;">
    <header class="banner">
        <!-- banner stuff -->
    </header>



    <main>
		<h1 id="title">Welcome to the Test Database! Use this page to pass objects and test your code.</h1>
        <br>

        <% if(typeof test == "undefined"){}else{  %>


            <p>The object passed to this page is: </p><br>
            <%= test.firstName + " " + test.lastName %>


            <table>
              <tr>
                <th>Name</th>
                <th>Rating</th>
                <th>Form Stuff</th>
              </tr>
              <% items.forEach(function(element){ %>
              <tr>
                <td> <%= element.itemName %></td>
                <td> <%= element.itemRating %></td>
                <td> <%= element._id %></td>
              </tr>
              <%}); %>

            </table>
      <% } %>

    </main>

	<footer id="Validation" class="clear">
        <p>TEST &copy;</p>
		<a href="https://validator.w3.org/" target="_blank">Validate HTML</a>
		<a href="https://jigsaw.w3.org/css-validator/check/referer" target="_blank">Validate CSS</a>
	</footer>

</body>
</html>
